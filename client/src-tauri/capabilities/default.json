{
  "identifier": "default",
  "description": "Haven default capabilities",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "updater:default",
    "process:default",
    "dialog:default",
    "fs:default",
    "fs:allow-create",
    "fs:allow-write",
    "fs:allow-open",
    "core:resources:allow-close",
    "fs:scope-home-recursive",
    "fs:scope-desktop-recursive",
    "fs:scope-download-recursive",
    "fs:scope-document-recursive"
  ]
}
